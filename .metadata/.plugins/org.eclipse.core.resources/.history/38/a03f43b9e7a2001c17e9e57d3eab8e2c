package com.devsuperior.dsmovie.entities;

public class Movie {
	private Long id;
	private String title;
	private Double score;
	private String image;
	
	public Movie() {
		
	}

	public Movie(Long id, String title, Double score, String image) {
		this.id = id;
		this.title = title;
		this.score = score;
		this.image = image;
	}
	

}
